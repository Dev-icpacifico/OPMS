CREATE TABLE "django_migrations" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "app" varchar(255) NOT NULL, "name" varchar(255) NOT NULL, "applied" datetime NOT NULL)
CREATE TABLE sqlite_sequence(name,seq)
CREATE TABLE "auth_group_permissions" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "group_id" integer NOT NULL REFERENCES "auth_group" ("id") DEFERRABLE INITIALLY DEFERRED, "permission_id" integer NOT NULL REFERENCES "auth_permission" ("id") DEFERRABLE INITIALLY DEFERRED)
CREATE TABLE "auth_user_groups" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "user_id" integer NOT NULL REFERENCES "auth_user" ("id") DEFERRABLE INITIALLY DEFERRED, "group_id" integer NOT NULL REFERENCES "auth_group" ("id") DEFERRABLE INITIALLY DEFERRED)
CREATE TABLE "auth_user_user_permissions" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "user_id" integer NOT NULL REFERENCES "auth_user" ("id") DEFERRABLE INITIALLY DEFERRED, "permission_id" integer NOT NULL REFERENCES "auth_permission" ("id") DEFERRABLE INITIALLY DEFERRED)
CREATE TABLE "django_admin_log" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "object_id" text NULL, "object_repr" varchar(200) NOT NULL, "action_flag" smallint unsigned NOT NULL CHECK ("action_flag" >= 0), "change_message" text NOT NULL, "content_type_id" integer NULL REFERENCES "django_content_type" ("id") DEFERRABLE INITIALLY DEFERRED, "user_id" integer NOT NULL REFERENCES "auth_user" ("id") DEFERRABLE INITIALLY DEFERRED, "action_time" datetime NOT NULL)
CREATE TABLE "django_content_type" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "app_label" varchar(100) NOT NULL, "model" varchar(100) NOT NULL)
CREATE TABLE "auth_permission" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "content_type_id" integer NOT NULL REFERENCES "django_content_type" ("id") DEFERRABLE INITIALLY DEFERRED, "codename" varchar(100) NOT NULL, "name" varchar(255) NOT NULL)
CREATE TABLE "auth_group" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "name" varchar(150) NOT NULL UNIQUE)
CREATE TABLE "auth_user" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "password" varchar(128) NOT NULL, "last_login" datetime NULL, "is_superuser" bool NOT NULL, "username" varchar(150) NOT NULL UNIQUE, "last_name" varchar(150) NOT NULL, "email" varchar(254) NOT NULL, "is_staff" bool NOT NULL, "is_active" bool NOT NULL, "date_joined" datetime NOT NULL, "first_name" varchar(150) NOT NULL)
CREATE TABLE "area_profesion" ("id_area_profesion" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre_area_profesion" varchar(100) NOT NULL)
CREATE TABLE "nacionalidade" ("id_nacionalidad" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre_nacionalidad" varchar(100) NOT NULL)
CREATE TABLE "profesione" ("id_profesione" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre_profesione" varchar(100) NOT NULL, "id_area_profesion_id" integer NOT NULL REFERENCES "area_profesion" ("id_area_profesion") DEFERRABLE INITIALLY DEFERRED)
CREATE TABLE "condominio" ("id_condominio" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre_condominio" varchar(100) NOT NULL, "alias_condominio" varchar(50) NOT NULL, "fecha_venta_condominio" date NULL, "direccion_proyecto" varchar(100) NOT NULL, "estado_condominio" varchar(30) NOT NULL, "empresa_vende_id" integer NULL REFERENCES "empresas" ("id_empresa") DEFERRABLE INITIALLY DEFERRED)
CREATE TABLE "modelo" ("id_modelo" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre_modelo" varchar(4) NOT NULL, "programa_modelo" varchar(10) NOT NULL, "estado_modelo" varchar(30) NOT NULL)
CREATE TABLE "etapa_proyecto" ("id_etapa_condominio" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre_etapa" varchar(50) NOT NULL, "id_condominio_id" integer NOT NULL REFERENCES "condominio" ("id_condominio") DEFERRABLE INITIALLY DEFERRED)
CREATE TABLE "sub_etapa_proyecto" ("id_sub_etapa" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre_sub_etapa" varchar(4) NOT NULL, "etapa_id" integer NOT NULL REFERENCES "etapa_proyecto" ("id_etapa_condominio") DEFERRABLE INITIALLY DEFERRED)
CREATE TABLE "torre" ("id_torre" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "estado_torre" varchar(30) NOT NULL, "nombre_torre" varchar(50) NOT NULL, "id_etapa_condominio_id" integer NOT NULL REFERENCES "etapa_proyecto" ("id_etapa_condominio") DEFERRABLE INITIALLY DEFERRED)
CREATE TABLE "etapas" ("id_etapa" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre_etapa" varchar(50) NULL, "alias_etapa" varchar(50) NULL, "descripcion_etapa" text NULL, "tipo_venta_asociado" varchar(50) NULL)
CREATE TABLE "campo_etapas" ("id_campo_etapa" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre_campo" varchar(50) NULL, "tipo_dato" varchar(50) NULL, "obligatorio" bool NOT NULL, "descripcion_campo" text NULL, "id_etapa_id" integer NOT NULL REFERENCES "etapas" ("id_etapa") DEFERRABLE INITIALLY DEFERRED)
CREATE TABLE "venta_etapas" ("id_venta_etapa" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "fecha_inicio" date NULL, "fecha_fin" date NULL, "id_etapa_id" integer NOT NULL REFERENCES "etapas" ("id_etapa") DEFERRABLE INITIALLY DEFERRED, "id_venta_id" integer NOT NULL REFERENCES "venta" ("id_venta") DEFERRABLE INITIALLY DEFERRED)
CREATE TABLE "valores_etapas" ("id_valores_etapa" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "valor_campo" varchar(50) NULL, "id_campo_etapa_id" integer NOT NULL REFERENCES "campo_etapas" ("id_campo_etapa") DEFERRABLE INITIALLY DEFERRED, "id_venta_etapa_id" integer NOT NULL REFERENCES "venta_etapas" ("id_venta_etapa") DEFERRABLE INITIALLY DEFERRED)
CREATE TABLE "banco" ("id_banco" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre_banco" varchar(50) NOT NULL)
CREATE TABLE "categoria_pago" ("id_categoria_pago" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre_categoria_pago" varchar(50) NOT NULL)
CREATE TABLE "forma_pago" ("id_forma_pago" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre_forma_pago" varchar(50) NOT NULL)
CREATE TABLE "pagos" ("id_pago" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "estado_pago" varchar(50) NOT NULL, "num_documento" varchar(50) NOT NULL, "fecha_registro_pago" date NOT NULL, "fecha_real_pago" date NOT NULL, "monto_pago" integer NOT NULL, "observacion_pago" varchar(100) NOT NULL, "id_banco_id" integer NOT NULL REFERENCES "banco" ("id_banco") DEFERRABLE INITIALLY DEFERRED, "id_categoria_pago_id" integer NOT NULL REFERENCES "categoria_pago" ("id_categoria_pago") DEFERRABLE INITIALLY DEFERRED, "id_forma_pago_id" integer NOT NULL REFERENCES "forma_pago" ("id_forma_pago") DEFERRABLE INITIALLY DEFERRED, "id_venta_id" integer NOT NULL REFERENCES "venta" ("id_venta") DEFERRABLE INITIALLY DEFERRED, "uf_pago" real NULL)
CREATE TABLE "modelo_vacio" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "atributo" varchar(100) NOT NULL)
CREATE TABLE "django_session" ("session_key" varchar(40) NOT NULL PRIMARY KEY, "session_data" text NOT NULL, "expire_date" datetime NOT NULL)
CREATE TABLE "clientes" ("id_cliente" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "tipo_cliente" varchar(50) NOT NULL, "rut_cliente" varchar(50) NOT NULL, "fecha_nacimiento" date NOT NULL, "genero" varchar(30) NOT NULL, "region" varchar(31) NOT NULL, "correo" varchar(254) NOT NULL, "telefono" varchar(9) NOT NULL, "renta" integer NOT NULL, "id_nacionalidad_id" integer NOT NULL REFERENCES "nacionalidade" ("id_nacionalidad") DEFERRABLE INITIALLY DEFERRED, "id_profesion_id" integer NOT NULL REFERENCES "profesione" ("id_profesione") DEFERRABLE INITIALLY DEFERRED, "apellidos_1" varchar(50) NOT NULL, "apellidos_2" varchar(50) NOT NULL, "nombres_1" varchar(50) NOT NULL, "nombres_2" varchar(50) NOT NULL)
CREATE TABLE "venta" ("id_venta" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "fecha_venta" date NOT NULL, "estado_venta" varchar(50) NOT NULL, "fecha_promesa" date NOT NULL, "ejecutivo" varchar(50) NULL, "bono_pie" real NULL, "uf_por_m2" real NULL, "id_cliente_id" integer NOT NULL REFERENCES "clientes" ("id_cliente") DEFERRABLE INITIALLY DEFERRED, "id_propiedad_id" integer NOT NULL REFERENCES "propiedades" ("id_propiedad") DEFERRABLE INITIALLY DEFERRED, "atributos" text NULL, "ggoo" integer NULL, "motivo_compra" text NULL, "tipo_venta" varchar(50) NOT NULL, "aplicacion_bono" varchar(10) NOT NULL, "descuento_campagna" varchar(2) NOT NULL, "uf_descuento_campagna" real NOT NULL, "precio_venta" real NOT NULL, "credito_hipotecario" real NULL, "saldo_contado" real NULL, "recursos_propio" real NULL, "subsidio" real NULL)
CREATE TABLE "gestion_postventa_alcanceresponsabilidad" ("id_alcanceresponsabilidad" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "descripcion" varchar(200) NOT NULL)
CREATE TABLE "gestion_postventa_causa" ("id_causa" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "descripcion" varchar(200) NOT NULL)
CREATE TABLE "gestion_postventa_especialidad" ("id_especialidad" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre" varchar(100) NOT NULL)
CREATE TABLE "gestion_postventa_item" ("id_item" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre" varchar(100) NOT NULL)
CREATE TABLE "gestion_postventa_lugar" ("id_lugar" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre" varchar(100) NOT NULL)
CREATE TABLE "gestion_postventa_problema" ("id_problema" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "descripcion" varchar(255) NOT NULL)
CREATE TABLE "gestion_postventa_recinto" ("id_recinto" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "nombre" varchar(100) NOT NULL)
CREATE TABLE "requerimientos_postventa" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "fecha" date NOT NULL, "solicitud_cliente" text NOT NULL, "lugar_id" integer NULL REFERENCES "gestion_postventa_lugar" ("id_lugar") DEFERRABLE INITIALLY DEFERRED, "item_id" integer NULL REFERENCES "gestion_postventa_item" ("id_item") DEFERRABLE INITIALLY DEFERRED, "problema_id" integer NULL REFERENCES "gestion_postventa_problema" ("id_problema") DEFERRABLE INITIALLY DEFERRED, "especialidad_id" integer NULL REFERENCES "gestion_postventa_especialidad" ("id_especialidad") DEFERRABLE INITIALLY DEFERRED, "estado" varchar(30) NOT NULL, "causa_id" integer NULL REFERENCES "gestion_postventa_causa" ("id_causa") DEFERRABLE INITIALLY DEFERRED, "alcance_responsabilidad_id" integer NULL REFERENCES "gestion_postventa_alcanceresponsabilidad" ("id_alcanceresponsabilidad") DEFERRABLE INITIALLY DEFERRED, "mo" real NOT NULL, "materiales" real NOT NULL, "subcontrato" real NOT NULL, "total" real NOT NULL, "comentario" text NULL, "propiedad_id" integer NOT NULL REFERENCES "propiedades" ("id_propiedad") DEFERRABLE INITIALLY DEFERRED, "recinto_id" integer NULL REFERENCES "gestion_postventa_recinto" ("id_recinto") DEFERRABLE INITIALLY DEFERRED)
CREATE TABLE "valor_uf" ("id_valor_uf" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "fecha_registro" date NOT NULL, "valor_uf" real NOT NULL)
CREATE TABLE "propiedades" ("id_propiedad" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "estado_propiedad" varchar(50) NOT NULL, "rol" varchar(50) NOT NULL, "numero_propiedad" varchar(3) NOT NULL, "piso" varchar(30) NOT NULL, "metros_vivienda" real NOT NULL, "estacionamiento" varchar(5) NOT NULL, "bodega" varchar(5) NOT NULL, "m2_bodega" varchar(5) NOT NULL, "valor_cchc" real NULL, "condominio_id" integer NOT NULL REFERENCES "condominio" ("id_condominio") DEFERRABLE INITIALLY DEFERRED, "etapa_id" integer NOT NULL REFERENCES "etapa_proyecto" ("id_etapa_condominio") DEFERRABLE INITIALLY DEFERRED, "modelo_id" integer NOT NULL REFERENCES "modelo" ("id_modelo") DEFERRABLE INITIALLY DEFERRED, "bono_propiedad" real NULL, "metros_terraza_propiedad" real NOT NULL, "metros_total_propiedad" real NOT NULL, "ori_propiedad" varchar(30) NOT NULL, "prorrateo_propiedad" real NOT NULL, "valor_final_propiedad" real NULL, "valor_inicial_propiedad" real NULL, "fpm" integer NULL, "torre_id" integer NOT NULL REFERENCES "torre" ("id_torre") DEFERRABLE INITIALLY DEFERRED, "rol_bodega" varchar(50) NOT NULL, "rol_estacionamiento" varchar(50) NOT NULL, "valor_bodega" real NOT NULL, "valor_estacionamiento" real NOT NULL)
CREATE TABLE "empresas" ("id_empresa" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "razon_social" varchar(50) NOT NULL, "rut_empresa" varchar(50) NOT NULL, "gerente_ventas" varchar(50) NOT NULL, "jefe_operaciones" varchar(50) NOT NULL, "banco_alzante" varchar(50) NOT NULL, "notaria" varchar(50) NOT NULL, "banco_transf" varchar(50) NOT NULL, "tipo_cuenta_transf" varchar(50) NOT NULL, "num_cuenta_transf" varchar(50) NOT NULL, "correo_transf" varchar(50) NOT NULL, "representante" varchar(50) NOT NULL, "rut_representante" varchar(50) NOT NULL)
